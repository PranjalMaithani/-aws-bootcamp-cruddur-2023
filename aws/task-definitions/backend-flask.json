{
    "family": "backend-flask",
    "containerDefinitions": [
        {
            "name": "backend-flask",
            "image": "975049922162.dkr.ecr.ap-south-1.amazonaws.com/backend-flask",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "backend-flask",
                    "containerPort": 4567,
                    "hostPort": 4567,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "OTEL_SERVICE_NAME",
                    "value": "backend-flask"
                },
                {
                    "name": "AWS_DEFAULT_REGION",
                    "value": "ap-south-1"
                },
                {
                    "name": "BACKEND_URL",
                    "value": "*"
                },
                {
                    "name": "AWS_COGNITO_USER_POOL_ID",
                    "value": "ap-south-1_GOXnOznDN"
                },
                {
                    "name": "AWS_COGNITO_USER_POOL_CLIENT_ID",
                    "value": "5b6ro31g97urk767adrbrdj1g5"
                },
                {
                    "name": "OTEL_EXPORTER_OTLP_ENDPOINT",
                    "value": "https://api.honeycomb.io"
                },
                {
                    "name": "FRONTEND_URL",
                    "value": "*"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "secrets": [
                {
                    "name": "AWS_ACCESS_KEY_ID",
                    "valueFrom": "arn:aws:ssm:ap-south-1:975049922162:parameter/cruddur/backend-flask/AWS_ACCESS_KEY_ID"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:ssm:ap-south-1:975049922162:parameter/cruddur/backend-flask/AWS_SECRET_ACCESS_KEY"
                },
                {
                    "name": "CONNECTION_URL",
                    "valueFrom": "arn:aws:ssm:ap-south-1:975049922162:parameter/cruddur/backend-flask/CONNECTION_URL"
                },
                {
                    "name": "ROLLBAR_ACCESS_TOKEN",
                    "valueFrom": "arn:aws:ssm:ap-south-1:975049922162:parameter/cruddur/backend-flask/ROLLBAR_ACCESS_TOKEN"
                },
                {
                    "name": "OTEL_EXPORTER_OTLP_HEADERS",
                    "valueFrom": "arn:aws:ssm:ap-south-1:975049922162:parameter/cruddur/backend-flask/OTEL_EXPORTER_OTLP_HEADERS"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "cruddur",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "backend-flask"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "python /bin/flask/health-check"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::975049922162:role/CruddurTaskRole",
    "executionRoleArn": "arn:aws:iam::975049922162:role/CruddurServiceExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}

